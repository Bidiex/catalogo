{
    "rewrites": [
        {
            "source": "/login",
            "destination": "/login.html"
        },
        {
            "source": "/dashboard",
            "destination": "/dashboard.html"
        },
        {
            "source": "/catalog",
            "destination": "/catalog.html"
        },
        {
            "source": "/product",
            "destination": "/product.html"
        },
        {
            "source": "/reset-password",
            "destination": "/reset-password.html"
        },
        {
            "source": "/update-password",
            "destination": "/update-password.html"
        },
        {
            "source": "/auth/callback",
            "destination": "/auth-callback.html"
        },
        {
            "source": "/c/:slug",
            "destination": "/catalog.html"
        },
        {
            "source": "/(.*)",
            "destination": "/404.html"
        }
    ]
}